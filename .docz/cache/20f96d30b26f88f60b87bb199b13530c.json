kground: 'none',\n        border: border.thin,\n        color: color,\n      },\n\n      accent: {\n        color: '#fff',\n        background: color.accent,\n        border: border.thinTransparent,\n      },\n      'primary-accent': {\n        color: color['pink-500'],\n        background: color['pink-200'],\n        border: border.thinTransparent,\n      },\n      'secondary-accent': {\n        background: 'none',\n        border: `1px solid ${color['pink-200']}`,\n        color: color['pink-200'],\n      },\n\n      action: {\n        color: color['light-blue-500'],\n        background: color['light-blue-200'],\n        border: border.thinTransparent,\n      },\n      'primary-action': {\n        color: '#fff',\n        background: color.action,\n        border: border.thinTransparent,\n      },\n      'secondary-action': {\n        background: 'none',\n        border: `1px solid ${color['light-blue-200']}`,\n        color: color['light-blue-200'],\n      },\n\n      danger: {\n        color: '#fff',\n        background: color.danger,\n        border: border.thinTransparent,\n      },\n      'primary-danger': {\n        color: color['red-500'],\n        background: color['red-200'],\n        border: border.thinTransparent,\n      },\n      'secondary-danger': {\n        background: 'none',\n        border: `1px solid ${color['red-200']}`,\n        color: color['red-200'],\n      },\n\n      success: {\n        color: '#fff',\n        background: color.success,\n        border: border.thinTransparent,\n      },\n      'primary-success': {\n        color: color['green-500'],\n        background: color['green-200'],\n        border: border.thinTransparent,\n      },\n      'secondary-success': {\n        background: 'none',\n        border: `1px solid ${color['green-200']}`,\n        color: color['green-200'],\n      },\n\n      warning: {\n        color: '#fff',\n        backgroundColor: color.warning,\n        border: border.thinTransparent,\n      },\n      'primary-warning': {\n        color: color['yellow-500'],\n        backgroundColor: color['yellow-200'],\n        border: border.thinTransparent,\n      },\n      'secondary-warning': {\n        background: 'none',\n        border: `1px solid ${color['yellow-200']}`,\n        color: color['yellow-200'],\n      },\n    }[key] || ''\n  )\n}\n\nexport function badgePillTheme({ pill, theme: { color } }) {\n  return pill\n    ? {\n        paddingRight: ' 0.6em',\n        paddingLeft: '0.6em',\n        borderRadius: '10rem',\n      }\n    : ''\n}\n"]}]}