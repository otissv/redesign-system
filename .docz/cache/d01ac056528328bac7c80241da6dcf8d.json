DF;AAKD;;cApBeF,iB;;;;;;;;;;;;;;;;;;;;;;0BARHJ,gB;0BAIGG,Y;0BAIAC,iB","sourcesContent":["import React, { useContext, useMemo, useState } from 'react';\n\nimport { AccordionInterface, AccordionContextType } from './accordion.types';\n\nexport const AccordionContext = React.createContext<AccordionContextType>({\n  active: '',\n});\n\nexport function useAccordion() {\n  return useContext(AccordionContext);\n}\n\nexport function AccordionProvider({\n  animate,\n  children,\n  active,\n}: AccordionInterface) {\n  const state = useState(active);\n\n  const context = useMemo(\n    () => ({\n      active: state[0],\n      animate,\n      setActiveItem: state[1],\n    }),\n    [state]\n  );\n  return (\n    <AccordionContext.Provider value={context}>\n      {children}\n    </AccordionContext.Provider>\n  );\n}\n"]}]}