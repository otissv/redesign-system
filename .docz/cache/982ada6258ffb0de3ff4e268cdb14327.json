tAlignTheme(props),\n    ...typographyTransformTheme(props),\n    ...sharedStyles()(props),\n  }\n}\n\nconst Link = styled.a`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst Abbr = styled.abbr`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst Code = styled.code`\n  ${getStyles};\n  ${typographyElementTheme};\n  ${props => props.styled};\n`\n\nconst Del = styled.del`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst Dfn = styled.dfn`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst Em = styled.em`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst H1 = styled.h1`\n  ${getStyles};\n  ${props => props.styled};\n`\nconst H2 = styled.h2`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst H3 = styled.h3`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst H4 = styled.h4`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst H5 = styled.h5`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst H6 = styled.h6`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst Hr = styled.hr`\n  ${getStyles};\n  ${typographyElementTheme} ${props => props.styled};\n`\n\nconst Ins = styled.ins`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst Q = styled.q`\n  ${getStyles};\n  ${typographyElementTheme} ${props => props.styled};\n`\n\nconst Mark = styled.mark`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst P = styled.p`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst Sm = styled.small`\n  ${getStyles};\n  ${typographyElementTheme};\n  ${props => props.styled};\n`\n\nconst Span = styled.span`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nconst Strong = styled.strong`\n  ${getStyles};\n  ${props => props.styled};\n`\n\nexport function Typography(props) {\n  const { theme } = useContext(ThemeContext)\n\n  const element = {\n    a: props => (\n      <Link href={props.href} {...props}>\n        {props.children}\n      </Link>\n    ),\n\n    abbr: props => <Abbr {...props}>{props.children}</Abbr>,\n\n    code: props => <Code {...props}>{props.children}</Code>,\n\n    del: props => <Del {...props}>{props.children}</Del>,\n\n    dfn: props => (\n      <Dfn {...props} title={props.title}>\n        {props.children}\n      </Dfn>\n    ),\n\n    em: props => <Em {...props}>{props.children}</Em>,\n\n    h1: props => <H1 {...props}>{props.children}</H1>,\n\n    h2: props => <H2 {...props}>{props.children}</H2>,\n\n    h3: props => <H3 {...props}>{props.children}</H3>,\n\n    h4: props => <H4 {...props}>{props.children}</H4>,\n\n    h5: props => <H5 {...props}>{props.children}</H5>,\n\n    h6: props => <H6 {...props}>{props.children}</H6>,\n\n    hr: props => <Hr {...props} />,\n\n    ins: props => <Ins {...props}>{props.children}</Ins>,\n\n    mark: props => <Mark {...props}>{props.children}</Mark>,\n\n    q: props => <Q {...props}>{props.children}</Q>,\n\n    p: props => <P {...props}>{props.children}</P>,\n\n    sm: props => <Sm {...props}>{props.children}</Sm>,\n\n    span: props => <Span {...props}>{props.children}</Span>,\n\n    strong: props => <Strong {...props}>{props.children}</Strong>,\n  }\n\n  const Component = element[props.element] || element['p']\n\n  return <Component theme={setTheme(props.theme, theme)} {...props} />\n}\n\nTypography.propTypes = {\n  context: PropTypes.oneOf([\n    'accent',\n    'action',\n    'danger',\n    'disabled',\n    'success',\n    'warning',\n  ]),\n  href: PropTypes.string,\n  textWrap: PropTypes.oneOf(['truncate', 'break', 'nowrap']),\n  element: PropTypes.oneOf([\n    'a',\n    'abbr',\n    'code',\n    'del',\n    'dfn',\n    'em',\n    'h1',\n    'h2',\n    'h3',\n    'h4',\n    'h5',\n    'h6',\n    'hr',\n    'ins',\n    'mark',\n    'q',\n    'p',\n    'sm',\n    'span',\n    'strong',\n  ]),\n  textAlign: PropTypes.oneOf([\n    'left',\n    'leftSmall',\n    'leftMedium',\n    'leftLarge',\n    'right',\n    'rightSmall',\n    'rightMedium',\n    'rightLarge',\n    'center',\n    'centerSmall',\n    'centerMedium',\n    'centerLarge',\n    'justify',\n  ]),\n\n  theme: PropTypes.object,\n}\n\nTypography.defaultProps = {\n  element: 'p',\n}\n\nexport const typography = styled(Typography)\nexport default Typography\n// textVertical='top'\tAligns text to the top.\n// textVertical='middle'\tCenters text vertically.\n// textVertical='bottom'\tAligns text to the bottom.\n//   }\n// }\n"]}]}